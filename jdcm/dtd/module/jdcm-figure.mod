<!-- Figure

       zu
-->

<!-- zu: Figure, photo, etc. with its margin, caption, note, etc.
     [JIS X 4052:2000, 6.7]  -->

<!ENTITY % jdcm.zu.element "INCLUDE">
<![%jdcm.zu.element;[
<!ENTITY % jdcm.zu.content "EMPTY">
<!ENTITY % jdcm.zu.qname "zu">
<!ENTITY % jdcm.zu.tagmin.start "-">
<!ENTITY % jdcm.zu.tagmin.end "o">
<![%sgml.tag.minimizable;[
<!ENTITY % jdcm.zu.tagmin "%jdcm.zu.tagmin.start; %jdcm.zu.tagmin.end;">
<!-- end of sgml.tag.minimizable -->]]>
<!ENTITY % jdcm.zu.tagmin "">
<!ELEMENT %jdcm.zu.qname; %jdcm.zu.tagmin; %jdcm.zu.content;>
<!-- end of jdcm.zu.element -->]]>

<!ENTITY % jdcm.zu.attlist "INCLUDE">
<![%jdcm.zu.attlist;[
<!ATTLIST %jdcm.zu.qname;
	src	%URI.datatype;	#REQUIRED
	%jdcm.subbody.attrib;
	%jdcm.common.attrib;>
<!-- end of jdcm.zu.attlist -->]]>


